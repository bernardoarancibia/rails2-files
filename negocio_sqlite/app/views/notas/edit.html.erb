<h1>Editing nota</h1>

<% form_for(@nota) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :vendedor_id %><br />
    <%= f.text_field :vendedor_id %>
  </p>
  <p>
    <%= f.label :texto %><br />
    <%= f.text_area :texto %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @nota %> |
<%= link_to 'Back', notas_path %>